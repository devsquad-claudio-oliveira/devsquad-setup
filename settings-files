echo "1. Install .php-cs-fixer.php"
curl https://raw.githubusercontent.com/iammayron/devsquad-setup/master/.php-cs-fixer.php -Ls > .php-cs-fixer.php

echo "2. Install phpcs.xml"
curl https://raw.githubusercontent.com/iammayron/devsquad-setup/master/phpcs.xml -Ls > phpcs.xml

echo "3. Install phpmd.xml"
curl https://raw.githubusercontent.com/iammayron/devsquad-setup/master/phpmd.xml -Ls > phpmd.xml

echo "4. Install phpunit.xml"
curl https://raw.githubusercontent.com/iammayron/devsquad-setup/master/phpunit.xml -Ls > phpunit.xml

echo "5. Install phpunit-ci.xml"
curl https://raw.githubusercontent.com/iammayron/devsquad-setup/master/phpunit-ci.xml -Ls > phpunit-ci.xml
